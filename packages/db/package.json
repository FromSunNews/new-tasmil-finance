{
  "name": "@repo/db",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "pnpm build --watch",
    "build": "tsc -b -v",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx src/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:pull": "drizzle-kit pull"
  },
  "main": "./dist/entry.js",
  "types": "./dist/entry.d.ts",
  "files": [
    "./dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/entry.js",
      "require": "./dist/entry.js"
    },
    "./*": {
      "import": "./dist/*.js",
      "require": "./dist/*.js"
    }
  },
  "dependencies": {
    "ai": "6.0.0-beta.159",
    "bcrypt-ts": "^5.0.2",
    "drizzle-orm": "^0.34.0",
    "postgres": "^3.4.4",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^22.10.7",
    "drizzle-kit": "^0.25.0",
    "dotenv": "^16.4.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.1",
    "typescript": "5.5.4"
  }
}

